    </formulas>
    <formulas>
        <name>forMatchStatus</name>
        <dataType>String</dataType>
        <expression>IF(AND(NOT(ISBLANK({!varSoldToAccount})),NOT(ISBLANK({!varProduct}))),&quot;Success&quot;,
IF(AND(ISBLANK({!varSoldToAccount}),ISBLANK({!varProduct})),&quot;Error: account and product cannot be matched!&quot;,
IF(AND(NOT(ISBLANK({!varSoldToAccount})),ISBLANK({!varProduct})),&quot;Error: product cannot be matched!&quot;,
IF(AND(ISBLANK({!varSoldToAccount}),NOT(ISBLANK({!varProduct}))),&quot;Error: account cannot be matched!&quot;,&quot;Error!&quot;))))</expression>
    </formulas>
